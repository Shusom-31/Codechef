#include <iostream>
using namespace std;

int main() {
	int a,b,x,y;
    scanf("%d%d%d%d",&a,&b,&x,&y);
      int m=(a*2)+b;
      int r=(x*2)+y;
      
      if(m<r){
          printf("Ronaldo");
      }else if (m>r)
      printf("Messi");
      else{
          printf("Equal");
      }
	return 0;
}
